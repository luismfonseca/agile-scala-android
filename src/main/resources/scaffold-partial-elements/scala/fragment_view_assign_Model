    val CLASS_NAME_UNCAPITALIZEDFIELD_NAME_CAPITALIZEDView = inflater.inflate(R.layout.fragment_view_FIELD_TYPE_UNDERSCORED, container, false)

    val FIELD_NAME_AS_ISFrameLayout = CLASS_NAME_UNCAPITALIZEDFrameLayout.findViewById(R.id.CLASS_NAME_UNDERSCORED_FIELD_NAME_UNDERSCORED_container).asInstanceOf[FrameLayout]
    FIELD_NAME_AS_ISFrameLayout.addView(CLASS_NAME_UNCAPITALIZEDFIELD_NAME_CAPITALIZEDView)

